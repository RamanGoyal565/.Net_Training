builder.Services.AddScoped<StudentRepository>();
@model IEnumerable<WebApplication_With_ADO.NET.Models.Student>

<h2>Students List</h2>

<table border="1" cellpadding="8">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
        <th>Square</th>
    </tr>

    @foreach (var s in Model)
    {
        <tr>
            <td>@s.Id</td>
            <td>@s.Name</td>
            <td>@s.Age</td>
            <td>@s.Square</td>
        </tr>
    }
</table>